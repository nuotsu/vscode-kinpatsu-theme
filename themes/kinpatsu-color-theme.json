{
  "type": "dark",
  "colors": {
    "editor.background": "#111",
    "editor.lineHighlightBorder": "#fff0",
    "editor.lineHighlightBackground": "#ffffff08",
    "editorLineNumber.foreground": "#555",
    "editorLightBulb.foreground": "#ee8f2f",
    "editorGroup.dropBackground": "#ee8f2f44",
    "editorGroupHeader.tabsBackground": "#181818",
    "tab.border": "#181818",
    "tab.activeBorder": "#ee8f2f",
    "tab.inactiveBackground": "#181818",
    "tab.hoverBackground": "#141414",
    "sideBar.background": "#181818",
    "sideBar.foreground": "#aaa",
    "statusBar.background": "#181818",
    "list.activeSelectionBackground": "#111",
    "list.activeSelectionForeground": "#f2b16b",
    "list.inactiveSelectionBackground": "#1119",
    "list.focusBackground": "#1119",
    "list.hoverBackground": "#111",
    "activityBar.background": "#111",
    "activityBar.foreground": "#f2b16b",
    "activityBar.activeBorder": "#ee8f2f",
    "activityBar.activeFocusBorder": "#ee8f2f",
    "activityBar.inactiveForeground": "#fff4",
    "activityBarBadge.background": "#ee8f2f",
    "progressBar.background": "#ee8f2f",
    "listFilterWidget.outline": "#ee8f2f",
  },
  "tokenColors": [
    // foreground
    {
      "name": "#555", "settings": { "foreground": "#555" },
      "scope": [
        "comment",
        "punctuation.definition.tag",
        "punctuation.section.property-list.begin.bracket",
        "punctuation.section.property-list.end.bracket",
        "punctuation.definition.dictionary.begin.json.comments",
        "punctuation.definition.dictionary.end.json.comments",
        "punctuation.definition.array.begin.json.comments",
        "punctuation.definition.array.end.json.comments",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx",
        "punctuation.definition.interpolation",
        "punctuation.definition.block",
        "punctuation.definition.template-expression",
        "punctuation.definition.dictionary.begin",
        "punctuation.definition.dictionary.end",
        "punctuation.definition.binding-pattern.object",
        "punctuation.accessor",
        "meta.brace.square",
        // .css, .scss
        "punctuation.section.function",
        "punctuation.definition.media-query.begin.bracket",
        "punctuation.definition.media-query.end.bracket",
        "punctuation.definition.attribute-selector.begin.bracket",
        "punctuation.definition.attribute-selector.end.bracket",
        // .md
        "punctuation.definition.heading.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.constant.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.markdown",
        "meta.separator.markdown",
      ],
    },
    {
      "name": "#888", "settings": { "foreground": "#888" },
      "scope": [
        "keyword",
        "storage",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.entity",
        // .md
        "punctuation.definition.list.begin.markdown",
      ]
    },
    {
      "name": "#f0f0f0", "settings": { "foreground": "#f0f0f0" },
      "scope": [
        "meta.structure.dictionary",
        "meta.object-literal.key",
        "support.type.property-name.json.comments",
        "variable.object.property",
        // .css, .scss
        "meta.property-name",
        "support.type.property-name",
        // .md
        "markup.heading.markdown",
      ]
    },

    {
      "name": "#ee8f28", "settings": { "foreground": "#ee8f28" },
      "scope": [
        "entity.name",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "variable.other.readwrite.alias",
        // .css, .scss
        "keyword.other.unit",
      ]
    },
    {
      "name": "#f2b16b", "settings": { "foreground": "#f2b16b" },
      "scope": [
        "string.quoted",
        "string.template",
        "entity.name.type",
        "constant.language.boolean",
        "constant.language.json",
        "support.type.primitive",
        // .css, .scss
        "variable.scss",
        "variable.scss",
        "support.constant.property-value",
        "support.function.misc",
        "meta.property-value.media-query",
        "constant.numeric",
        "constant.other.color.rgb-value",
      ]
    },

    // fontStyle
    {
      "name": "italic", "settings": { "fontStyle": "italic" },
      "scope": [
        "keyword.control",
        "storage",
        "comment",
        "constant.language.boolean",
        "constant.language.json.comments",
        "keyword.operator.optional",
        "support.type.primitive",
        // .md
        "markup.italic.markdown",
        "markup.quote.markdown",
      ]
    },
    {
      "name": "bold", "settings": { "fontStyle": "bold" },
      "scope": [
        "support.class.component",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        // .md
        "markup.heading.markdown",
        "markup.bold.markdown",
      ]
    },
    {
      "name": "normal", "settings": { "fontStyle": "" },
      "scope": [
        "storage.type.function.arrow",
        // .md
        "markup.underline.link.markdown",
      ]
    }
  ]
}
